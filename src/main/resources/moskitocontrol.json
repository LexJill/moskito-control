{
    "mailNotificationEnabled" : false,
    "notificationsMutingTime": 60,

  "@components": [
    {
      "name": "Web",
      "category": "web",
      "connectorType": "HTTP",
      "location": "http://10.111.54.26:8080"
    },
    {
      "name": "Controller",
      "category": "web",
      "connectorType": "HTTP",
      "location": "http://10.111.54.26:8081"
    },
    {
      "name": "Cron",
      "category": "web",
      "connectorType": "HTTP",
      "location": "http://10.111.54.26:8082"
    },

    {
      "name": "accountservice",
      "category": "service",
      "connectorType": "RMI",
      "location": "biz.test.mo:9260"
    },
    {
      "name": "accountadminservice",
      "category": "service",
      "connectorType": "RMI",
      "location": "biz.test.mo:9261"
    },
    {
      "name": "accountsettingsservice",
      "category": "service",
      "connectorType": "RMI",
      "location": "biz.test.mo:9262"
    },
    {
      "name": "authenticationservice",
      "category": "service",
      "connectorType": "RMI",
      "location": "biz.test.mo:9263"
    },
    {
      "name": "foreignidservice",
      "category": "service",
      "connectorType": "RMI",
      "location": "biz.test.mo:9264"
    }
  ],
  "@charts": [

    {
      "name": "Sessions",
      "limit": 100,
      "@lines": [
        {
          "component": "*",
          "accumulator": "SessionCount Cur Absolute"
        }
      ]
    },

    {
      "name": "CPU",
      "limit": 100,
      "@lines": [
        {
          "component": "*",
          "accumulator": "CPU Time 1m"
        }
      ]
    },
    {
      "name": "URL AVG",
      "limit": 100,
      "@lines": [
        {
          "component": "*",
          "accumulator": "URL AVG 1m"
        }
      ]
    },
    {
      "name": "URL REQ",
      "limit": 100,
      "@lines": [
        {
          "component": "*",
          "accumulator": "URL REQ 1m"
        }
      ]
    },
    {
      "name": "Process CPU Load",
      "limit": 100,
      "@lines": [
        {
          "component": "*",
          "accumulator": "Process CPU Load 1m"
        }
      ]
    },
    {
      "name": "System CPU Load",
      "limit": 100,
      "@lines": [
        {
          "component": "*",
          "accumulator": "System CPU Load 1m"
        }
      ]
    },
    {
      "name": "Memory",
      "limit": 20000,
      "@lines": [
        {
          "component": "*",
          "accumulator": "Mem OldGenFree MB 1m"
        },
        {
          "component": "*",
          "accumulator": "Mem OldGenUsed MB 1m"
        }
      ]
    }


  ],

    "@connectors": [
        {
            "type": "HTTP",
            "className": "org.moskito.control.connectors.HttpConnector"
        },
        {
            "type": "RMI",
            "className": "org.moskito.control.connectors.RMIConnector"
        }
    ],

    "@statusUpdater": {
        "checkPeriodInSeconds": 10,
        "threadPoolSize": 10,
        "timeoutInSeconds": 60,
        "enabled": true
    },

    "@chartsUpdater": {
        "checkPeriodInSeconds": 40,
        "threadPoolSize": 5,
        "timeoutInSeconds": 60,
        "enabled": true
    },
	"@pluginsConfig":{
	    "@plugins": [
	    ]
	} 


}
